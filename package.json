  "license": "ISC",
  "main": "gulpfile.js",
  "scripts": {
    "start": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zlojkashtan/WebpageSkeleton.git"
  },
  "keywords": [
    "gulp",
    "boilerplate"
  ],
  "config": {
    "dev": true,
    "src": {
      "templates": "src/templates",
      "sass": "src/sass",
      "css_img": "src/sass/img",
      "fonts": "src/sass/fonts",
      "js": "src/js",
      "jslibs": "src/js-libs",
      "img": "src/images"
    },
    "build": {
      "html": "build/",
      "css": "build/css",
      "css_img": "build/css/img",
      "fonts": "build/css/fonts",
      "js": "build/js",
      "img": "build/images"
    },
    "web": {
      "html": "templates/",
      "css": "templates/styles",
      "css_img": "templates/styles/img",
      "fonts": "templates/styles/fonts",
      "js": "templates/scripts",
      "img": "templates/images"
    },
    "banner": [
      "/**",
      " * Autor: Yurii Skoriiantov",
      " * Email: zlojkashtan@gmail.com",
      " */",
      ""
    ]
  },
  "devDependencies": {
    "gulp": "^3.9.1"
  },
  "dependencies": {
    "browser-sync": "^2.24.7",
    "bs-console-qrcode": "^1.0.2",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-changed": "^3.2.0",
    "gulp-cheerio": "^0.6.3",
    "gulp-clean-css": "^3.10.0",
    "gulp-concat": "^2.6.1",
    "gulp-front-matter": "^1.3.0",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-header": "^2.0.5",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^2.0.2",
    "gulp-image": "^4.3.0",
    "gulp-jshint": "^2.1.0",
    "gulp-notify": "^3.2.0",
    "gulp-nunjucks-render": "^2.2.2",
    "gulp-plumber": "^1.2.0",
    "gulp-prettify": "^0.5.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.1",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-svgmin": "^2.1.0",
    "gulp-uglify": "^3.0.1",
    "gulp-watch": "^5.0.1",
    "gulp.spritesmith": "^6.9.0",
